{
  "name": "dashboard-creator-server",
  "private": false,
  "version": "3.2.4",
  "license": "MIT",
  "main": "src/server.ts",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start:prod": "node dist/server.js",
    "start:dev": "ts-node-dev --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "test": "jest -c jest.config.js --env=node",
    "test:database-info": "jest -c jest.config.js --env=node ./src/components/db-api/databaseInfo.test.js",
    "test:dashboard": "jest -c jest.config.js --env=node ./src/components/dashboard/dashboard.test.ts",
    "test:cov": "jest -c jest.config.js --coverage --env=node",
    "test:watch": "jest -c jest.config.js -i --watchAll",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "prettier": "prettier --check \"{*.js,!(node*)**/*.ts}\"",
    "prettier:fix": "prettier --write \"{*.js,!(node*)**/*.ts}\"",
    "loglevel:change": "runtime-node-refresh",
    "preload-data:blockchains": "ts-node -r tsconfig-paths/register src/components/blockchain/blockchain.script.ts"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.21.0",
    "@polkadot/api": "^10.11.2",
    "@polkadot/keyring": "^12.6.2",
    "@polkadot/util": "^12.6.2",
    "@subsquid/ink-abi": "^3.0.3",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.3",
    "apicache": "^1.6.3",
    "axios": "^1.6.4",
    "cors": "^2.8.5",
    "deepmerge": "^4.3.1",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.1",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-mongo-sanitize": "^2.2.0",
    "fs": "^0.0.1-security",
    "http-status": "^1.5.0",
    "jest-mock-axios": "^4.7.3",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "md5": "^2.3.0",
    "mongodb-memory-server": "^9.1.6",
    "mongoose": "^7.0.5",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "node-mocks-http": "^1.11.0",
    "path": "^0.12.7",
    "runtime-node-refresh": "^1.0.6",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "uuid": "^9.0.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "^2.5.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0"
  }
}
